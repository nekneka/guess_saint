<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >

    <!--auto Next-->
    <CheckBoxPreference
        android:defaultValue="false"
        android:key="autoNext"
        android:summaryOff="@string/settings_auto_next_off"
        android:summaryOn="@string/settings_auto_next_on"
        app:iconSpaceReserved="false"
        android:title="@string/settings_auto_next" />

    <Preference
        android:key="settings_reset_db"
        android:title="@string/settings_reset_option_title"
        android:summary="@string/settings_reset_option_summary"
        app:iconSpaceReserved="false"
        >

    </Preference>

</PreferenceScreen>
